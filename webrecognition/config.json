{
  "name": "WebRecognition",
  "version": "1.0",
  "panel_icon": "mdi:face-recognition",
  "slug": "webrecognition",
  "description": "Face recognition local web server",
  "url": "https://github.com/AndreiRadchenko/hassio-webrecognition",
  "startup": "application",
  "boot": "auto",
  "map": [ 
    "addons:rw",
    "backup:rw",
    "config:rw",
    "media:rw",
    "share:rw"
  ],
  "webui": "http://[HOST]:[PORT:5001]/",
  "watchdog": "http://[HOST]:[PORT:5001]/",
  "ingress": true,
  "ingress_port": 5001,
  "panel_admin": true,
  "ports": {
    "5001/tcp": 5001
  },
  "ports_description": {
    "5001/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "host_network": true,
  "devices": [
  ],
  "usb": false,
  "tmpfs": false,
  "full_access": false,
  "options": {},
  "schema": {},
  "arch": [
    "amd64",
    "armv7"
  ]
}
