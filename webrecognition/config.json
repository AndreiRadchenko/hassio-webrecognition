{
  "name": "WebRecognition",
  "version": "1.0",
  "panel_icon": "mdi:face-recognition",
  "slug": "webrecognition",
  "description": "Face recognition local web server",
  "url": "https://github.com/AndreiRadchenko/hassio-webrecognition",
  "ingress": true,
  "ingress_port": 5001,
  "init": true,
  "ports": {
    "5001/tcp": 5001
  },
  "map": [
    "examples:rw",
    "examples/data:rw"
  ],
  "ports_description": {
    "5001/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "boot": "auto",
  "environment": {
    "CONFIG_FILE": "/config/webrecognition.yml"
  },
  "options": {},
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "agent_secret": "str?"
  },
  "arch": [
    "amd64",
    "armv7"
  ]
}
